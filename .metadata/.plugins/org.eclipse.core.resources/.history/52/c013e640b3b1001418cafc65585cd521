import java.util.Comparator;

import org.omg.CORBA.PRIVATE_MEMBER;
import org.omg.CORBA.PUBLIC_MEMBER;


public class MergeSort {

	private void merge(Object[] a, Object[] aux, int low, int mid, int high, Comparator c) {
		for (int k = low; k <= high; k++)
			aux[k] = a[k];
		
		int i = low, j = mid + 1;
		for (int k = low; k <= high; k++){
			if (i > mid)
				a[k] = aux[j++];
			else if (j > high)
				a[k] = aux[i++];
			else if (c.compare(aux[j], aux[i]) < 0)
				a[k] = aux[j++];
			else 
				a[k] = aux[i++];
		}
	}
	
	public void sort(Object[] a, Object[] aux, int low, int high, Comparator c) {
		if(low >= high)
			return;
		int mid = (low + high) / 2;
		sort(a, aux, low, mid, c);
		sort(a, aux, mid+1, high, c);
		merge(a, aux, low, mid, high, c);
	}
	
	private class Item(int id, String name) {
		
		public static final Comparator<Item> BY_ID = new ById();
		public static final Comparator<Item> BY_NAME = new ByName();
		
		private int id;
		private String name;
		
		public Item (int id, String name) {
			this.id = id;
			this.name = name;
		}
		
		private static class ById implements Comparator<Item>{
			@Override
			public int compare(Item o1, Item o2) {
				return o1.id - o1.id;
			}
		}
		
		private static class ByName implements Comparator<Item> {
			@Override
			public int compare(Item o1, Item o2) {
				return o1.name.compareTo(o2.name);
			}
		}
	}
	
	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
